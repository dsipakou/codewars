def calculate(s):
    left = 0
    right = 0
    right_num = 0
    operation = ""
    while s[right].isdigit():
        right += 1
    left_num = int(s[left:right])
    while right <= len(s):
        print ("right", right)
        if s[right].isdigit():
            right += 1
        else:
            right_num = int(s[left:right])
            if operation:
                if operation == "minus":
                    left_num -= right_num
                else:
                    left_num += right_num
            if s[right] == "m":
                operation = "minus"
                left += 5
                right += 5
            else:
                operation = "plus"
                left += 4
                right += 4

print(calculate("896minus738"))
